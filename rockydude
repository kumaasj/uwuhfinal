wait(1)
if game.PlaceId == 606849621 then
    game:GetService("TeleportService"):Teleport(9780994092, game:GetService('Players').LocalPlayer)
end

local TextChatService = game:GetService('TextChatService')
local ChatSystem = TextChatService.TextChannels['RBXGeneral']
local msg_tbl = {
    'ðŸ”´á´¡á´€É´á´› Êá´á´œÊ€ á´…Ê€á´‡á´€á´ á´„á´€Ê€ êœ°á´Ê€ á´„Êœá´‡á´€á´˜ï¹– | VISIT JBsá´‡ÊŸÊŸá´‡Ê€s.sá´›á´Ê€á´‡',
    'ðŸ”´Ê™á´œÊ á´€É´Ê Éªá´›á´‡á´ Êá´á´œ á´¡á´€É´á´› | VISIT JBsá´‡ÊŸÊŸá´‡Ê€s.sá´›á´Ê€á´‡',
    'ðŸ”´Sá´€á´ á´‡ Êá´á´œÊ€ á´›Éªá´á´‡ á´€É´á´… Êá´á´œÊ€ á´¡á´€ÊŸÊŸá´‡á´› | VISIT JBsá´‡ÊŸÊŸá´‡Ê€s.sá´›á´Ê€á´‡',
    'ðŸ”´Wá´‡ á´€Ê€á´‡ á´›Êœá´‡ á´„Êœá´‡á´€á´˜á´‡sá´› á´€É´á´… á´›Êœá´‡ Ê™á´‡sá´›  | GO JBsá´‡ÊŸÊŸá´‡Ê€s.sá´›á´Ê€á´‡',
    'ðŸ”´Wá´€É´á´› á´›Êœá´‡ Ê™á´‡sá´› êœ°á´Ê€ á´„Êœá´‡á´€á´˜ï¹– | VISIT JBsá´‡ÊŸÊŸá´‡Ê€s.sá´›á´Ê€á´‡',
